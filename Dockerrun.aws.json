{
 "AWSEBDockerrunVersion": 2,
 "authentication": {
   "bucket": "<BUCKET>",
   "key": "dockercfg"
 },
 "volumes": [
   {
      "name": "app",
      "host": {
        "sourcePath": "/var/app/current/app"
      }
    }
 ],
 "containerDefinitions": [
   {
     "name": "app",
     "image": "<IMAGE>:<TAG>",
     "essential": true,
     "memory": 128,
     "portMappings": [
       {
         "hostPort": 80,
         "containerPort": 80
       }
     ],
     "mountPoints": [
        {
          "sourceVolume": "app",
          "containerPath": "/var/www/html",
          "readOnly": true
        }
     ]
   }
 ]
}